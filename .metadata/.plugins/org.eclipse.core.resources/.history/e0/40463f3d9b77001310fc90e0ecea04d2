package solutions.ws13.assignment8;

public class costCalculation {

	int costs;
	
	public costCalculation() {
		costs = 0;
	}
	
	public void addExpense(int expense) {
		costs -= expense;
	}
	
	public void addIncome(int income) {
		costs += income;
	}
	
	public int getCosts() {
		return costs;
	}

}
